{
  "name": "kwcay-eslint",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "qa": "yarn format && yarn lint --fix",
    "publish-rules": "yarn qa && yarn publish packages/eslint-config-kwcay-rules",
    "publish-kwcay": "yarn qa && yarn publish packages/eslint-config-kwcay",
    "publish-typescript": "yarn qa && yarn publish packages/eslint-config-kwcay-typescript"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "prettier": "2.3.0"
  }
}
